object VarDeclTest {
  fun test() {
    ## assign string to int variable in declaration
    var a Int = ""

    ## assign Float to int variable in declaration
    var b Int = 30.40

    ## assign Int to Float variable in declaration
    var c Float = 3

    ## assign bool to variable of type String
    var x String = 10

    ## declaration without value
    var y Bool

    ## declaration without type
    var z = 10

    ## parameterized type Array
    var ary Array<Int>
  }
}
